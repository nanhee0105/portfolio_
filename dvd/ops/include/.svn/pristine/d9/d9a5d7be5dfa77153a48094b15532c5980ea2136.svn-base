
/* 잠깐 안전 */
.Safety {
  position: absolute;
  bottom: 0px;
  left: 0;
  width: 570px;
  height: 91px;
  border-radius: 0 100px 0 0;
  background-color: #ffdd7e;
}
.Safety .safetyTop {
  position: absolute;
  top: -40px;
  width: 165px;
  height: 46px;
  background: url(../../images/btn/safe.png) no-repeat center/contain;
}
.Safety .content {
  padding: 20px 0px 10px 20px;
}
.Safety .content p {
  position: relative;
  left: 18px;
  width: 480px;
  margin-bottom: 7px;
  font-size: 20px;
}
.Safety .content p::after {
  content: "";
  position: absolute;
  top: 7px;
  left: -20px;
  width: 10px;
  height: 10px;
  border-radius: 100%;
  background-color: #fff;
  box-shadow: 3px 3px 1px #ddbf6e;
}
.Safety .content p:nth-last-of-type(1) {
  margin-bottom: 0;
}
.Safety .slideUp {
  position: absolute;
  top: -10px;
  right: 70px;
  width: 24px;
  height: 24px;
  background: url(../../images/btn/safel_up.png) no-repeat center top/100%;
}
.Safety .slideUp.ON {
  background: url(../../images/btn/safel_up.png) no-repeat center bottom/100%;
}

.Safety .slideDown {
  background: url(../../images/btn/safe_down.png) no-repeat center top/100%;
}
.Safety .slideDown.ON {
  background: url(../../images/btn/safe_down.png) no-repeat center bottom/100%;
}


.line3 { animation: line3 2s linear forwards; }
.line4 { animation: line4 2s linear forwards; }
.line5 { animation: line5 2s linear forwards; }
.linedefault { animation: linedefault 2s linear forwards; }
@keyframes line3 {
  0% {height:91px}
  10% {height: 124px;}
  100% {height: 124px;}
}
@keyframes line4 {
  0% {height:91px}
  10% {height: 157px;}
  100% {height: 157px;}
}
@keyframes line5 {
  0% {height:91px}
  10% {height: 188px;}
  100% {height: 188px;}
}

@keyframes linedefault {
  0% {height:auto}
  10% {height: 91px;}
  100% {height: 91px;}
}
