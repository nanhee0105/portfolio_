@charset "utf-8";

body {
	background-color: transparent;
}

/* íƒ‘ */
.topMenu{
	position: absolute;
	top:0px;
	left:0px;
	height:52px;
	width:100%;
	line-height:52px;
	background: #006699;
	box-sizing: border-box;
	box-shadow: 0px 1px 2px 2px #ccc;
	/* z-index: 1; */
}
.topMenu .menutext{
	font-size:18px;
	position: absolute;
	left: 75px;
	top: 0px;
	width: 650px;
	color: #FFF;
	line-height: 52px;
	font-family: "NotoSansKR-Light";
}
.isShow .topMenu > div {
	opacity: .2;
	pointer-events: none;
}
.topMenu .menutext span{
	font-size:16px;
	font-family: "NotoSansKR-Medium";
}
.topMenu .menutext span.titleColor{
	color: #ffffbd;
}
.topMenu img{
	height: 53px;
	position: absolute;
	left: 0px;
	top: 0px;
	cursor: pointer;
	vertical-align:text-bottom;
	pointer-events: auto !important;
}
.noStepTextInHeader .stepArea {
	display: none;
}

.pagebox {
	position: absolute;
	right: 164px;
	top: 10px;
}
.pagebox .pageBtnright{
	width: 25px;
	height: 25px;
	background: url(../../../images/nav/pageBtnright_on.png) no-repeat center top/100%;
	cursor: pointer;
	position: absolute;
	right: 5px;
	top: 6px;
}
.pagebox .pageBtnright.ON{
	background: url(../../../images/nav/pageBtnright_on.png) no-repeat center bottom/100%;
}
.pagebox .pageBtnright.OFF{
	background: url(../../../images/nav/pageBtnright_off.png) no-repeat center top/contain;
	pointer-events: none;
}

.pagebox .pageBtnleft{
	width: 25px;
	height: 25px;
	background: url(../../../images/nav/pageBtnleft_on.png) no-repeat center top/100%;
	cursor: pointer;
	position: absolute;
	left: 5px;
	top: 6px;
}
.pagebox .pageBtnleft.ON{
	background: url(../../../images/nav/pageBtnleft_on.png) no-repeat center bottom/100%;
}
.pagebox .pageBtnleft.OFF{
	background: url(../../../images/nav/pageBtnleft_off.png) no-repeat center top/contain;
	pointer-events: none;
}
.pagebox .pagenum {
	width: 145px;
	height: 35px;
	border-radius: 25px;
	background: #fff;
}
.pagebox .pagenum p{
	width: 95px;
	height: auto;
	font-size: 20px;
	position: absolute;
	left: 26px;
	top: 0px;
	color: #ff9933;
}
.pagebox .pagenum p input[type="text"]{
	width: 42px;
	font-size: 18px;
	background: transparent;
	vertical-align: baseline;
	color: #000;
	font-family: "NotoSansKR-Bold";
}

.help {
	background: url(../../../images/nav/help.png) no-repeat center top/100%;
	width: 30px;
	height: 25px;
	position: absolute;
	top: 16px;
	right: 119px;
	cursor: pointer;
}
.help.ON {
	background: url(../../../images/nav/help.png) no-repeat center bottom/100%;
}
.help:before {
	content: "";
	height: 23px;
	color: #006d33;
	position: absolute;
	top: 0px;
	left: -7px;
	border: 1px dashed;
}
.pagetext{
	position: absolute;
	top: 9px;
	right: 320px;
	width: 235px;
	height: 35px;
	color: #111;
	line-height:35px;
	text-align: center;
	border-radius: 20px;
	background: #ffd865;
	cursor: pointer;
}
.pagetext span:nth-child(1) {
	color: #ff6d33;
}
.pagetext p,.pagetext span{
	font-size: 18px;
	height: 35px;
	line-height: 35px;
}
.appbox{
	width: 30px;
	height: 25px;
	background: url(../../../images/nav/topmenudot2.png) no-repeat center top/100%;
	cursor: pointer;
	position: absolute;
	right: 68px;
	top: 16px;
}
.appbox.ON {
	background: url(../../../images/nav/topmenudot2.png) no-repeat center bottom/100%;
}
.appbox:before{
	content: "";
	height: 23px;
	color: #006d33;
	position: absolute;
	top: 0px;
	left: -15px;
	border: 1px dashed;
}
.mainCloseBtn{
	width: 30px;
	height: 25px;
	background: url(../../../images/nav/mainCloseBtn.png) no-repeat center top/100%;
	position:absolute;
	top: 16px;
	right: 20px;
	cursor: pointer;
}
.mainCloseBtn.ON {
	background: url(../../../images/nav/mainCloseBtn.png) no-repeat center bottom/100%;
}
.mainCloseBtn:before {
	content: "";
	height: 23px;
	color: #006d33;
	position: absolute;
	top: 0px;
	left: -7px;
	border: 1px dashed;
}


/*****************appList*********************/
.appWarp{
	width: 86px;
	height: 570px;
	position: absolute;
	top: 50px;
	right: 39px;
	background:url(../../../images/nav/appbox.png) no-repeat center top/contain;
	display:none;
}
.appWarp ul{
	cursor: default;
}
.appWarp .mainApp {
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	right: 0;
}
.mainApp div.appList {
	width: 69px;
	height: 462px;
	position: absolute;
	top: 58px;
	left: 8px;
}
.mainApp div.appList > div {
	width: 100%;
}
.mainApp div.appList li {
	height: 77px;
}
.mainApp div.appList > div:nth-child(1) {}

.mainApp div.appList > div:nth-child(1) li {}
.mainApp p.appArea {
	width: 100%;
	height: 59px;
	cursor: pointer;
}

.dictionary p.appArea {
	background: url(../../../images/nav/app_dictionary.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.lab p.appArea {
	background: url(../../../images/nav/app_lab.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.pen p.appArea {
	background: url(../../../images/nav/app_pen.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.board p.appArea {
	background: url(../../../images/nav/app_board.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.timer p.appArea {
	background: url(../../../images/nav/app_timer.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.sticker p.appArea {
	background: url(../../../images/nav/app_sticker.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.announce p.appArea {
	background: url(../../../images/nav/app_announce.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.stamp p.appArea {
	background: url(../../../images/nav/app_stamp.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.reference p.appArea {
	background: url(../../../images/nav/app_lab.png) no-repeat;
	background-position: center top;
	background-size: 63px;
}
.appWarp.on {
	display: block;
	z-index: 1;
}
.mainApp p.appArea.on,
.mainApp p.appArea:hover {
	background-position: center bottom;
}


/* stamp */
.stampWrap {
	display: none;
	position: absolute;
	top: 52px;
	left: 0;
	width: 1280px;
	height: calc(720px - 52px);
	overflow: hidden;
	z-index: -1;
}
.stampWrap.on {
	display: block;
}

.stampDropArea {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	z-index: -1;
}
.cursorImg {
	position: absolute;
	width: 155px;
	height: 167px;
	transform: translate(-77px, -130px);
	background: url("../../../images/nav/stampCursor.png") no-repeat center top/contain;
	cursor: pointer;
	pointer-events: none;
	z-index: 0;
}
.cursorImg.eraser {
	width: 60px;
	height: 60px;
	transform: translate(-40px, -70px);
	background: url("../../../images/nav/stamp_erase_02.png") no-repeat center/contain;
}
.cursorImg.hide {
	opacity: 0;
}

.stampDropArea .stampImage {
	position: absolute;
	width: 200px;
	height: 200px;
	background: center / contain no-repeat;
	pointer-events: auto;
	z-index: -1;
}
.stampDropArea .stampImage[data-type="1"] {
	background-image: url("../../../images/nav/stamp1_result.png");
}
.stampDropArea .stampImage[data-type="2"] {
	background-image: url("../../../images/nav/stamp2_result.png");
}
.stampDropArea .stampImage[data-type="3"] {
	background-image: url("../../../images/nav/stamp3_result.png");
}
.stampDropArea .stampImage[data-type="4"] {
	background-image: url("../../../images/nav/stamp4_result.png");
}

.stampBox {
	position: absolute;
	top: 20px;
	right: 20px;
	width: 307px;
	height: 60px;
	background: url("../../../images/nav/stamp_bg.png") no-repeat center top/contain;
	z-index: 1;
}

.stampBox .stamp_drag {
	display: block;
	width: 33px;
	height: 33px;
	background: url(../../../images/nav/stamp_drag.png) no-repeat center top/contain;
	position: absolute;
	top: 18px;
	left: 7px;
	cursor: pointer;
}

.stampBox .stamp_close {
	display: block;
	width: 21px;
	height: 22px;
	background: url(../../../images/nav/stamp_close.png) no-repeat center top;
	background-size: 22px 44px;
	position: absolute;
	top: 0px;
	left: 285px;
	overflow: hidden;
	cursor: pointer;
}

.stampBox .stamp_close:hover {
	background-position: center bottom;
}

.stampBox .stampArea {
	box-sizing: content-box;
	width: 156px;
	height: 45px;
	position: absolute;
	bottom: 2px;
	left: 55px;
}

.stampBox .stampArea > div {
	float: left;
	width: 39px;
	height: 45px;
}

.stampBox .stampArea > div > p {
	width: 36px;
	height: 36px;
	margin-top: 3px;
}
.stampBox .appArea {
	background: no-repeat center/28px 28px;
}
.stampBox .stamp1 .appArea {
	background-image: url("../../../images/nav/stamp1.png");
}

.stampBox .stamp2 .appArea {
	background-image: url("../../../images/nav/stamp2.png");
}

.stampBox .stamp3 .appArea {
	background-image: url("../../../images/nav/stamp3.png");
}

.stampBox .stamp4 .appArea {
	background-image: url("../../../images/nav/stamp4.png");
}

.stampBox .stamp5 .appArea {
	background-image: url("../../../images/nav/stamp5.png");
}

.stampArea .on .appArea,
.stampArea .appArea:hover {
	background-size: 36px 36px;
}

.easerArea {
	box-sizing: content-box;
	width: 68px;
	height: 45px;
	position: absolute;
	bottom: 3px;
	right: 13px;
}

.easerArea > div {
	width: 34px;
	height: 45px;
	float: left;
}

.stampBox .easerArea > div > p {
	width: 34px;
	height: 45px;
}

.stampBox .easerArea .easer1 .appArea {
	background: url("../../../images/nav/stamp_erase_01.png") no-repeat center/25px;
}

.stampBox .easerArea .easer2 .appArea {
	background: url("../../../images/nav/stamp_erase_02.png") no-repeat center/25px;
}

.stampBox .easerArea .appArea.on,
.stampBox .easerArea .appArea:hover {
	background-size: 34px;
}





