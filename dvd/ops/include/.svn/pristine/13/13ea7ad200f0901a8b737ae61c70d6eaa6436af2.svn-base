/***************.gnb***********************/
.isShow .gnb {
	animation: slidedown .3s ease-in-out;
}
.gnb{
	position: relative;
	left: calc(50% - (1350px * 0.92 / 2));
	width: calc(1350px * 0.95);
	height: 100px;
	padding: 30px 0px;
	box-sizing: border-box;
	/* transform: translateX(-50%); */
}
.gnb>li{
	float: left;
	position: relative;
	width: auto;
	padding: 0 20px;
}

.gnb h2 > img {
	width: 50px;
	margin-left: 10px;
	vertical-align: middle;
}
.gnb h2 {
	position: relative;
	left: 0px;
	top: 0px;
	width: 100%;
	height: 45px;
	color:#fff;
	font-size: 18px;
	font-family: "NotoSansKR-Bold";
	line-height: 45px;
	text-align: center;
	cursor: pointer;
	z-index:1;
}
.gnb h2 > img {
	width: 50px;
	vertical-align: middle;
	margin-left: 10px;
}
.gnb > li.ON {
	transform: scale(1.2);
	transition: all 0.4s ease-out 0s;
} 
.gnb > li.selected {
	pointer-events: none;
}
.gnb > li.selected h2 {
	color: #ffda26;
}

.liInner.selected {
	display: block;
	animation: slideleftLessonBox .4s ease-in-out;
}

@keyframes slideleftLessonBox {
	0% { transform: translateX(-300px); opacity: 0; }
	50% { transform: translateX(-300px); opacity: 0; }
	100% { transform: none; }
}

.liInner{
	display: none;
	position: absolute;
	top: 130px;
	left: 0;
	width: 250px;
	height: 460px;
	background: #ff9c33;
	border-radius: 0px 250px 250px 0px;
	overflow: hidden;
	z-index: 1;
}
.liInner .ofNum {
	display: flex;
	position: relative;
	top: 60px;
	left: 10px;
	width: 195px;
}
.liInner .ofNum span {
	color: #fff;
	font-size: 30px;
}

.liInner .ofNum span:nth-of-type(1){
	margin-right: 5px;
}

.liInner .subBox {
	position: relative;
	top: 69px;
	left: 30px;
	width: 190px;
}

.hasSubMenu .infoInner {
	display: block;
}
.infoInner {
	display: none;
}

.liInner h3{
	font-size: 17px;
	color: #a95800;
	font-family: "NotoSansKR-Bold";
	cursor: pointer;
	height: 44px;
	line-height:40px;
}
.infoInner .infoBox {
	display: none;
	position: absolute;
	top: 250px;
	left: 260px;
	width: auto;
	height: auto;
	max-width: 360px;
	text-align: left;
}
.infoInner .infoBox.selected {
	display: block;
	animation: slideleftLessonBox .6s ease-in-out;
}
.infoInner .infoBox::after {
	content: "";
	width: 70px;
	height: 450px;
	position: absolute;
	top: -80px;
	left: 330px;
	background: url(../../../images/nav/menu_line.png)no-repeat center/contain;
}

.infoInner .infoBox>div{
	display: flex;
	align-items: center;
	width: auto;
	min-height: 40px;
	padding: 4px 20px 8px;
	margin-bottom: 10px;
	color:#000;
	font-size: 18px;
	text-align: left;
	cursor: pointer;
}
.infoInner .infoBox>div.selected,
.infoInner .infoBox>div.ON {
	background-color: #ff9c33;
	border-radius: 20px;
	color: #fff;
}
.subBox.selected h3,
.subBox.ON h3 {
	color: #ffffa2;
}
.subBox h3:hover span,
.subBox h3 span.selected {
	background: #ffffa2;
}
.subBox>.infoBox>div:hover,
.subBox>.infoBox>div.selected{
	color:#000;
	background:url("../../../images/nav/white_bar.png") no-repeat 100px 3px/450px 35px;
}
.subBox > p.hidden {
	display: none;
}
.dashDot{
	display: inline-block;
	position: relative;
	left: -2px;
	top: -2px;
	width: 10px;
	height: 10px;
	margin-right: 4px;
	border-radius:50%;
	background: #fe9d32;
}

.liInner li.tapTitle >span:hover,
.liInner li.tapTitle >span.selected{
	color:#fff;
}
.liInner li.tapTitle >span:hover .dashDot,
.liInner li.tapTitle >span.selected .dashDot{
	background:#fff;
}











/********sectionInfo***************************/




.sectionInfo {
	position: absolute;
	top: 180px;
	left: 280px;
	width: 420px;
	height: 430px;
	/* z-index: 1; */
}

.hasSubMenu .sectionInfo {
	left: 660px;
	width: 560px;
}
.sectionInfo .infoArea{
}
.sectionInfo .infoArea.type2{
	top:150px;
}
.sectionInfo .infoPop{
	width: auto;
	height: auto;
	display:none;
}
.sectionInfo .infoPop.selected {
	display: block;
}
/* .sqDot{display: inline-block;} */

.sectionInfo .infoPop.selected .downArea {
	animation: showDownArea 1.0s ease-in-out;
}
.sectionInfo .infoPop.selected .infoArea {
	animation: showInfoArea .8s ease-in-out;
}

@keyframes showDownArea {
	0% {transform: translateY(100px); opacity: 0;}
	80% {transform: translateY(100px); opacity: 0;}
	100% {opacity: 1;}
	
}
@keyframes showInfoArea {
	0% {transform: translateX(-100px); opacity: 0;}
	70% {transform: translateX(-100px); opacity: 0;}
	100% {opacity: 1;}
}

.iconPoint{
	display: inline-block;
	width:30px;
	height:20px;
}
.subBox p{
	font-size:18px;
	cursor: pointer;
	height: auto;
	line-height: 25px;
	margin: 6px 0px 5px 17px;
}
.subBox p.selected,
.subBox p:hover {color: #ff6000;}
.noDownArea_pop .infoArea{
	top:40px;
}

