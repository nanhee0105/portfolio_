

/* 생각쑥쑥 */
.typeB .box {
  position: absolute;
  bottom: 0px;
  width: 100%;
  height: 600px;
  background-color: #fff;
  border-radius: 0 70px 0 0;
}
.typeB .title {
  position: absolute;
  top: -36px;
  width: 100%;
  height: 82px;
  background: url(../../images/common/sub/title_think.png) no-repeat left/361px 82px;
}
.typeB .title span {
  position: absolute;
  left: 260px;
  top: 8px;
  font-size: 32px;
  font-family: "cafe24";
}
.typeB .Text {
  position: relative;
  left: 125px;
  width: 1100px;
  margin: 50px 0 20px 0;
  height: auto;
}
.typeB .Text p::after {
  content: "";
  position: absolute;
  top: 20px;
  left: -35px;
  width: 20px;
  height: 20px;
  background-color: #0782c2;
  border-radius: 100%;
}
.typeB .Text_input {
  position: relative;
  left: 125px;
  width: 1100px;
  height: 320px;
  padding: 5px;
  text-align: center;
  vertical-align: middle;
  border: 2px solid #cdc3b5;
  box-sizing: border-box;
}