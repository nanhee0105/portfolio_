.typeA .contWrap {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 1150px;
  height:580px;
  transform: translate(-50%, -50%);
  background-color: #fff;
  border-radius: 30px;
}
.typeA .title {
  position: absolute;
  top: -30px;
  left: -30px;
  width: 190px;
  height: 187px;
  background: url(../../images/common/title_timeCheck_5.png) no-repeat center/contain;
}
.typeA .title span {
  position: absolute;
  top: 40px;
  width: 100%;
  color: #fff;
  font-size: 32px;
  font-family: "cafe24";
  text-align: center;
}
.typeA .Text {
  position: relative;
  left: 215px;
  top: 50px;
  width: 880px;
  height: auto;
  text-align: justify;
}
.typeA .Text p {
  position: relative;
  margin-bottom: 30px;
  text-align: justify;
}
.typeA .Text > p::after {
  content: "";
  position: absolute;
  top: 20px;
  left: -35px;
  width: 20px;
  height: 20px;
  background-color: #0782c2;
  border-radius: 100%;
}
.typeA .Text p:last-of-type(1) {
  margin-bottom: 0px;
}

